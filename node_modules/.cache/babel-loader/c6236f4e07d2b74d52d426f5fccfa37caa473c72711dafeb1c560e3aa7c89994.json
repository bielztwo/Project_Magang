{"ast":null,"code":"import { ref } from 'vue';\nexport default {\n  __name: 'Sidebar',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const expanded = ref(false);\n    const toggleMenu = () => {\n      expanded.value = !expanded.value;\n    };\n    const __returned__ = {\n      expanded,\n      toggleMenu,\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","expanded","toggleMenu","value"],"sources":["/home/cello/Documents/Vs_Code/Project_Magang/src/components/Sidebar.vue"],"sourcesContent":["<template>\n    <aside :class=\"`${expanded && 'expanded'}`\">\n        <div class=\"logo\">\n            <img src=\"../assets/logo.png\" alt=\"vue\">\n        </div>\n        <div class=\"toggle-wrap\">\n            <button class=\"toggle\">\n                <link rel=\"stylesheet\" href=\"https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@48,700,0,0\" />\n                <span class=\"material-symbols-outlined\">\n                keyboard_double_arrow_right\n                </span>\n            </button>\n        </div>\n    </aside>\n</template>\n\n<script setup>\nimport { ref }  from 'vue';\nconst expanded = ref(false);\nconst toggleMenu =()=>{\n    expanded.value = !expanded.value;\n}\n</script>\n\n<style lang=\"scss\" scoped>\naside{\n    display: flex;\n    flex-direction: column;\n    width: calc(2rem + 32px);\n    min-height: 100vh;\n    overflow: hidden;\n    padding: 1rem;\n    background-color: black;\n    color: var(--light);\n    transition: 0.2s ease-out;\n\n    .logo{\n        margin-bottom: 1rem;\n        img{\n            width: 2rem;\n        }\n    }\n\n    .toggle-wrap{\n        display: flex;\n        justify-content: flex-end;\n        margin-bottom: 1rem;\n\n        position:relative;\n        top: 0;\n        transition: 0.2s ease-out;\n        .toggle{\n            transition: 0.2s ease-out;\n            .material-icons{\n                font-size: 2rem;\n                color: var(--light);\n            }\n        }\n    }\n\n   \n    &.expanded{\n        width: var(--sidebar-width);\n\n        .toggle-wrap{\n            top: -3rem;\n            .toggle{\n                transform: rotate(180deg);\n            }\n        }\n    }\n    @media (max-width: 768px) {\n        position: fixed;\n        z-index: 99;\n    }\n}\n\n</style>"],"mappings":"AAiBA,SAASA,GAAG,QAAS,KAAK;;;;;;;IAC1B,MAAMC,QAAQ,GAAGD,GAAG,CAAC,KAAK,CAAC;IAC3B,MAAME,UAAU,GAAEA,CAAA,KAAI;MAClBD,QAAQ,CAACE,KAAK,GAAG,CAACF,QAAQ,CAACE,KAAK;IACpC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}